{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "coreclr",
      "request": "launch",
      "name": ".NET Core Launch (web)",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/routine-ly",
      "args": [],
      "cwd": "${workspaceFolder}/routine-ly",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart",
      "launchBrowser": {
        "enabled": false,
        "args": "${auto-detect-url}",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start ${auto-detect-url}"
        },
        "osx": {
          "command": "open"
        },
        "linux": {
          "command": "xdg-open"
        }
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://localhost:4200"
      },
      "sourceFileMap": {
        "/src/app": "${workplaceFolder}/src/app"
      }
    },
    {
      "type": "coreclr",
      "request": "attach",
      "name": ".NET Core Attach"
    }
  ]
}